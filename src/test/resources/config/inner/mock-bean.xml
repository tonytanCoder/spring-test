<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="customerCouponMapper" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.weipan.tradeservice.mapper.coupon.CustomerCouponMapper"></constructor-arg>
    </bean>
    <bean id="itemsMapper" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.weipan.tradeservice.mapper.items.ItemsMapper"></constructor-arg>
    </bean>
    <bean id="customerBalanceMapper" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.weipan.tradeservice.mapper.user.CustomerBalanceMapper"></constructor-arg>
    </bean>
</beans>